<!doctype html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="Adadain Quentin, Ardilouze TimothÃ©e, Dureau Ludovic, Erpeldinger Guillaume">
    <meta name="description" content="">
    <title>{% block title %}Job-Tech{% endblock %}</title>
    <link rel="shortcut icon" href="{{ asset('build/images/favicon.png') }}" type="image/x-icon">
    <link href="https://fonts.googleapis.com/css2?family=Baloo+2:wght@400;500;600;700;800&family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body>
<header>
    {% block header %}
    {% include 'components/_navbar.html.twig' %}
    {% endblock %}
</header>

<main>
    {% block body %}
    {% endblock %}
</main>

{% block footer %}
    {{ render(controller(
        'App\\Controller\\JobTechController::footer'
    )) }}
{% endblock %}

{% block javascript %}
    {{ encore_entry_script_tags('app') }}
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-173063700-1%22%3E"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-173063700-1');
    </script>
{% endblock %}
</body>

</html>
